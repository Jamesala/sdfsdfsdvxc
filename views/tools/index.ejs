                                            alert('Wiadomo≈õƒá zosta≈Ça wys≈Çana pomy≈õlnie!');
                                        } else {
                                            alert('B≈ÇƒÖd: ' + result.error);
                                        }
                                    } catch (error) {
                                        alert('WystƒÖpi≈Ç b≈ÇƒÖd podczas wysy≈Çania');
                                    }
